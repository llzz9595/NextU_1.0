<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/ic_maproom_sendlocation_background"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="25dp"
        android:orientation="vertical"
        >

            <!--<ImageView-->
                <!--android:layout_PaddingLeft="0dp"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="35dp"-->
                <!--android:src="@drawable/ic_maproom_sendlocation_search"-->
                <!--android:layout_gravity="center"-->
                <!--android:background="@drawable/corner_common_border_gray_left"-->
                <!--/>-->

    <AutoCompleteTextView
        android:drawableLeft="@drawable/ic_maproom_sendlocation_search"
        android:id="@+id/edit_maproom_Type"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:textSize="15dp"
        android:paddingLeft="3dp"
        android:hint="请输入你的辅导科目"
        android:background="@drawable/corner_common_border_gray"

        android:completionThreshold="1"
        />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"

            android:layout_marginTop="15dp"

            >
            <TextView
                android:id="@+id/txt_maproom_sendlocation_example_1"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="管理学"
                android:textColor="@color/green"
                android:gravity="center"
                android:layout_marginRight="15dp"
                />
            <TextView
                android:gravity="center"
                android:id="@+id/txt_maproom_sendlocation_example_2"
                android:layout_width="wrap_content"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_height="25dp"
                android:textColor="@color/green"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="计算机网络"
                android:layout_marginRight="15dp"
                />

            <TextView
                android:gravity="center"
                android:id="@+id/txt_maproom_sendlocation_example_3"
                android:layout_width="wrap_content"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_height="25dp"
                android:textColor="@color/green"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="国际金融"
                android:layout_marginRight="15dp"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_marginTop="10dp"

            >
            <TextView
                android:id="@+id/txt_maproom_sendlocation_example_5"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="会计学原理"
                android:textColor="@color/green"
                android:gravity="center"
                android:layout_marginRight="15dp"
                />
            <TextView
                android:gravity="center"
                android:id="@+id/txt_maproom_sendlocation_example_6"
                android:layout_width="wrap_content"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_height="25dp"
                android:textColor="@color/green"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="线性代数"
                android:layout_marginRight="15dp"
                />

            <TextView
                android:gravity="center"
                android:id="@+id/txt_maproom_sendlocation_example_7"
                android:layout_width="wrap_content"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_height="25dp"
                android:textColor="@color/green"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="13dp"
                android:text="统计学"
                android:layout_marginRight="15dp"
                />
            <TextView
                android:gravity="center"
                android:id="@+id/txt_maproom_sendlocation_example_8"
                android:layout_width="wrap_content"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_height="25dp"
                android:textColor="@color/green"
                android:background="@drawable/corner_common_border_green_blank"
                android:textSize="14dp"
                android:text="..."
                android:layout_marginRight="15dp"
                />

        </LinearLayout>

        <EditText
            android:layout_marginTop="15dp"
            android:id="@+id/edit_maproom_TypeDetail"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:textSize="15dp"
            android:hint=" 请输入科目描述，可具体到辅导章节"
            android:background="@drawable/corner_common_border_gray"

            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal">
        <TextView

            android:id="@+id/txt_maproom_sendlocation"
            android:layout_width="200dp"
            android:layout_height="34dp"
            android:background="@drawable/corner_common_border_green_4"
            android:gravity="center"
            android:text="我要辅导"
            android:textColor="#FEFEFE"
            android:textSize="14dp" />

        </LinearLayout>
    </LinearLayout>


</LinearLayout>